{
	"version": 3,
	"routes": [
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "^\\/_image$",
			"dest": "_render"
		},
		{
			"src": "^\\/404\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/dashboard\\/settings\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/dashboard\\/tasks\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/dashboard\\/([^/]+?)\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/dashboard\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/login\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/signup\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/$",
			"dest": "_render"
		},
		{
			"src": "/.*",
			"dest": "_render",
			"status": 404
		}
	]
}